{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfpro2"
		},
		"ADlLs_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADlLs'"
		},
		"AzDlLs_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzDlLs'"
		},
		"AzSaLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzSaLS'"
		},
		"AzSqlDbLs_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzSqlDbLs'"
		},
		"LsCustMasterOP_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LsCustMasterOP'"
		},
		"LsJoinsOp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LsJoinsOp'"
		},
		"LsOlapOp_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LsOlapOp'"
		},
		"ADlLs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakepro2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfpro2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADlLs_accountKey": {
						"value": "[parameters('ADlLs_accountKey')]"
					},
					"AzDlLs_connectionString": {
						"value": "[parameters('AzDlLs_connectionString')]"
					},
					"AzSaLS_connectionString": {
						"value": "[parameters('AzSaLS_connectionString')]"
					},
					"AzSqlDbLs_connectionString": {
						"value": "[parameters('AzSqlDbLs_connectionString')]"
					},
					"LsOlapOp_connectionString": {
						"value": "[parameters('LsOlapOp_connectionString')]"
					},
					"LsCustMasterOP_connectionString": {
						"value": "[parameters('LsCustMasterOP_connectionString')]"
					},
					"LsJoinsOp_connectionString": {
						"value": "[parameters('LsJoinsOp_connectionString')]"
					},
					"ADlLs_properties_typeProperties_url": {
						"value": "[parameters('ADlLs_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}